pragma solidity 0.8.17;


contract SumFunctions {
    function addsomething(uint a, uint b) public pure returns (uint) {
        return a + b;
    }

    function subSomething(uint a, uint b) public pure returns (uint) {
        return a + b;
    }

    function mutSomething(uint a, uint b) public pure returns (uint) {
        return a * b;
    }

    function divSomething(uint a, uint b) public pure returns (uint) {
        return a / b;
    }
}
